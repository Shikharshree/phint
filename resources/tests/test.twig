{{'<?php'}}

namespace {{testFqns}};

use {{classFqcn}};
use {{phpunit}} as TestCase;

{% set special = isTrait or isAbstract or isInterface %}
{% if special %}
/**
 * Wraps {{className}}
 */
{% endif %}
{% if isTrait %}
class Test{{className}}
{
    use {{className}};
}

{% elseif isInterface %}
class Test{{className}} implements {{className}}
{
    //
}

{% elseif isAbstract %}
class Test{{className}} extends {{className}}
{
    //
}

{% endif %}
/**
 * Auto generated by `phint test`
 */
class {{className}}Test extends TestCase
{
{% if (newable or special) and setup %}
    /**
     * @var {{ special ? 'Test' : ''}}{{className}}
     */
    protected ${{className|lcfirst}};

{% endif %}
{% if setup %}
    public function setUp()
    {
        parent::setUp();

{% if newable or special %}
        $this->{{className|lcfirst}} = new {{ special ? 'Test' : ''}}{{className}};
{% endif %}
    }
{% endif %}
{% if teardown %}

    public function tearDown()
    {
        parent::tearDown();
    }
{% endif %}
{% for method, attr in methods if not attr.abstract %}

{% if naming == 't' %}
    public function test{{method|ucfirst}}()
{% elseif naming == 'm' %}
    public function test_{{method|snake}}()
{% elseif naming == 'i' %}
    /**
     * @test
     */
    public function it_{{method}}()
{% endif %}
    {
{% if attr.static %}
        $actual = {{className}}::{{method}}();
{% elseif setup %}
        $actual = $this->{{className|lcfirst}}->{{method}}();
{% else %}
        ${{className|lcfirst}} = new {{ special ? 'Test' : ''}}{{className}};

        $actual = ${{className|lcfirst}}->{{method}}();
{% endif %}

        // $this->assertSame('', $actual);
    }
{% endfor %}
}
